
public class CounterThread extends Thread {

	private String name; 
	
	public public CounterThread(String name) {
		this.name = name;
	}
	
	@Override
	public void run() {
		for (int i = 0; i < 100; i++) {
			System.out.println(i);
			System.out.println(getClass().getName());
			try {
				Thread.sleep(1);
			} catch (InterruptedException e) {	}
		}		
	}
}
